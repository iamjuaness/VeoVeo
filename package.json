{
  "name": "veoveo",
  "private": true,
  "version": "2.1.1",
  "type": "module",
  "scripts": {
    "dev": "pnpm --parallel --filter @veoveo/web --filter @veoveo/server dev",
    "dev:web": "pnpm --filter @veoveo/web dev",
    "dev:server": "pnpm --filter @veoveo/server dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @veoveo/web build",
    "build:server": "pnpm --filter @veoveo/server build",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "preinstall": "npx only-allow pnpm"
  },
  "description": "**VeoVeo** is a modern web app for movie lovers who want to **track, organize, and discover movies** with ease. Built with **React**, **TypeScript**, and **Vite**, VeoVeo lets you manage your personal movie collection, track which films you've seen (and how many times), and create a watchlist for the future.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamjuaness/VeoVeo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamjuaness/VeoVeo/issues"
  },
  "homepage": "https://github.com/iamjuaness/VeoVeo#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "baseline-browser-mapping": "^2.8.30",
    "fix-esm-import-path": "^1.10.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/html2canvas": "^1.0.0",
    "@types/jspdf": "^2.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.544.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^2.15.4"
  }
}